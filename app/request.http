Content-Type: application/json

{
    "title": "linea modificada",
    "category": "GEOGRAPHY",
    "entries": [{
        "title": "title ",
        "text": "descripcion",
        "date": { 
            "year": 1922
        }
    },{
        "title": "other title",
        "text": "Hi",
        "date": { 
            "year": 2, 
            "ad": false
        }
    }]
}

